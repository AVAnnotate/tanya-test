      "uuid": "b6a33995-982e-4390-a286-1ccfa2476f80"
    },
    {
      "start_time": 1979.547574,
      "end_time": 1979.547574,
      "annotation": [
        {
          "text": "Alright."
        }
      ],
      "tags": [
        {
          "category": "_uncategorized_",
